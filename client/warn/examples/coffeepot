
on = 0

def f(x):
    global on
    if (x>200):
        on = 1
    if (x<50 and on == 1):
        on = 5
        return on
        on = 0

            
warn = [
    {
    'resource': "Kill-A-Watt.ilana-desktop.2:Watts", 
    'compare': lambda x: f(x) > 2,
